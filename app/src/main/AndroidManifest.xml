<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.example.p5_213"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">

        <activity
            android:name=".ui.StartActivity"
            android:exported="true"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".ui.MenuActivity" />
        <activity android:name=".ui.BurgerActivity" />
        <activity android:name=".ui.SandwichActivity" />
        <activity android:name=".ui.ComboActivity" />
        <activity android:name=".ui.SideActivity" />
        <activity android:name=".ui.BeverageActivity" />
        <activity android:name=".ui.CartActivity" />
        <activity android:name=".ui.ArchiveActivity" />

    </application>

</manifest>
